/*纸巾水纹*/
.main-circle {
  width: 200rpx;
  left: 50%;
  margin-left: -200rpx;
  height: 200rpx;
  position: fixed;
  bottom: 600rpx;
  position: -1;
}

.circle1,
.circle2,
.circle3,
.circle4 {
  width: 200px;
  height: 200px;
  border-radius: 50%;
  position: absolute;
  top: 20%;
  opacity: 0;
}

.circle1 {
  animation: myfirst 2s linear 0s;
  background-color: #FFBB59;
  -moz-animation: myfirst 2s linear 0s;
  -webkit-animation: myfirst 2s linear 0s;
  -o-animation: myfirst 2s linear 0s;
}

.circle2 {
  animation: myfirst 2s linear 0.5s;
  background-color: #FFBB59;
  -moz-animation: myfirst 2s linear 0.5s;
  -webkit-animation: myfirst 2s linear 0.5s;
  -o-animation: myfirst 2s linear 0.5s;
}

.circle3 {
  animation: myfirst 2s linear 1s;
  background-color: #FFBB59;
  -moz-animation: myfirst 2s linear 1s;
  -webkit-animation: myfirst 2s linear 1s;
  -o-animation: myfirst 2s linear 1s;
}

.circle4 {
  animation: myfirst 2s linear 1.5s;
  background-color: #FFBB59;
  -moz-animation: myfirst 2s linear 1.5s;
  -webkit-animation: myfirst 2s linear 1.5s;
  -o-animation: myfirst 2s linear 1.5s;
}

@keyframes myfirst {
  0% {
    -webkit-transform: scale(1);
    transform: scale(1);
    opacity: 1;
  }

  10% {
    -webkit-transform: scale(1.15);
    transform: scale(1.15);
    opacity: 0.9;
  }

  20% {
    -webkit-transform: scale(1.3);
    transform: scale(1.3);
    opacity: 0.8;
  }

  30% {
    -webkit-transform: scale(1.45);
    transform: scale(1.45);
    opacity: 0.7;
  }

  40% {
    -webkit-transform: scale(1.6);
    transform: scale(1.6);
    opacity: 0.6;
  }

  50% {
    -webkit-transform: scale(1.75);
    transform: scale(1.75);
    opacity: 0.5;
  }

  60% {
    -webkit-transform: scale(1.9);
    transform: scale(1.9);
    opacity: 0.4;
  }

  70% {
    -webkit-transform: scale(2.05);
    transform: scale(2.05);
    opacity: 0.3;
  }

  80% {
    -webkit-transform: scale(2.2);
    transform: scale(2.2);
    opacity: 0.2;
  }

  90% {
    -webkit-transform: scale(2.35);
    transform: scale(2.35);
    opacity: 0.1;
  }

  100% {
    -webkit-transform: scale(2.5);
    transform: scale(2.5);
    opacity: 0;
  }
}

